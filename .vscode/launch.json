    "version": "0.2.0",
    "configurations": [
        {
            "name": "Remote SPARQL",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "intavia_backend.main:app"
            ],
            "jinja": true,
            "justMyCode": true
        },
        {
            "name": "Local SPARQL",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app"
            ],
            "jinja": true,
            "justMyCode": true,
            "env": {
                "SPARQL_ENDPOINT": "http://127.0.0.1:8080/bigdata/sparql"
            }
        },
        {
            "name": "Dynamic Python: File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "justMyCode": true
        }
    ]
}